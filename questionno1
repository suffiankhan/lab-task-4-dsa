#include<iostream>
using namespace std;

void getfourelement(int arr[],int n)
{
    for(int i=0;i<4;i++)
    {
        int min=i;
        for(int j=i+1;j<n;j++)
        {
            if(arr[j] < arr[min])
            {
                min=j;
            }
        }
        int temp = arr[i];
        arr[i]= arr[min];
        arr[min]=temp;
    }
}

int main()
{
    int arr[10];
    for(int i=0;i<10;i++)
    {
        cin>>arr[i];
    }
    getfourelement(arr,10);
    for(int i=0;i<4;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
