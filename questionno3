#include<iostream>
#include<string>
using namespace std;

void sortthefruits(string arr[], int n)
{
    for(int i = 0; i < n-1; i++)
    {
        for(int j = 0; j < n-i-1; j++)
        {
            if(arr[j][0] > arr[j+1][0])
            {
                swap(arr[j], arr[j+1]);
            }
        }
    }
}

int main()
{
    int n;
    cout<<"Enter the number of fruits you want to enter :";
    cin>>n;
    string arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    sortthefruits(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}

