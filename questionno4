#include<iostream>
using namespace std;

bool isduplicatepresent(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==arr[i+1])
        {
            return true;
        }
    }
    return false;
}

int main()
{
    int n;
    cout<<"Enter the number of elements you want to enter :";
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }

    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-i-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                swap(arr[j],arr[j+1]);
            }
        }
    }

    bool check = isduplicatepresent(arr,n);
    if(check == true) cout<<"true"<<endl;
    else cout<<"false"<<endl;
    return 0;
}
