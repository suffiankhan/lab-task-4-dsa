
#include<iostream>
using namespace std;

void sortthearray(int arr[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-i-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                swap(arr[j],arr[j+1]);
            }
        }
    }
}

bool searchinthearray(int arr[],int n,int target)
{
    int start = 0;
    int end1 = n-1;
    while(start<=end1)
    {
        int mid=(end1+start)/2;
        if(arr[mid]==target)
        {
            return true;
        }
        else if(arr[mid] < target)
        {
            start=mid+1;
        }
        else
        {
            end1 = mid-1;
        }
    }
    return false;
}
int main()
{
    int n;
    cout<<"Enter the number of elements you want to enter :";
    cin>>n;
    int arr[100];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    sortthearray(arr,n);
    int target = 29;
    bool check = searchinthearray(arr,n,target);
    if(check) cout<<"true";
    else
    {
        int pos=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i] > target)
            {
                pos = i;
                break;
            }
        }

        for(int j=n;j>pos;j--)
        {
            arr[j]=arr[j-1];
        }
        arr[pos]=target;
        check = searchinthearray(arr,n,target);
        if(check) cout<<"true"<<endl;
        else cout<<"false"<<endl;

    }
    return 0;
}
